if let Some(&dp) = d.first().and_then(|inner_vec| inner_vec.get(0)) {
      let counts = [
        ca.iter().filter(|inner| inner.len() == 0).count(),
        ca.iter().filter(|inner| inner.len() == 1).count(),
        ca.iter().filter(|inner| inner.len() == 2).count(),
      ];
      if counts.iter().all(|&count| count <= 12 && count <= 13 && count <= 14) {
        dp
      }
      else {
        0
      }
  } else {
      0
  }